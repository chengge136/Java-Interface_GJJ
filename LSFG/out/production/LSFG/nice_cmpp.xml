<?xml version="1.0" encoding="utf-8"?>
<config>
	<!--与InfoX建立连接所需参数 -->
	<ismg>
		<!-- InfoX主机地址 -->
		<host>211.140.12.45</host>
		<!-- InfoX主机端口号 -->
		<port>7890</port>
		<!--(登录帐号SP…ID) -->
		<source-addr>578806</source-addr>
		<!--登录密码 -->
		<shared-secret>1dAZ4R!57</shared-secret>
		<!-- 心跳信息发送间隔时间(单位：秒) -->
		<heartbeat-interval>5</heartbeat-interval>
		<!-- 连接中断时重连间隔时间(单位：秒) -->
		<reconnect-interval>1</reconnect-interval>
		<!-- 物理连接读操作超时时间(单位：秒) -->
		<read-timeout>0</read-timeout>
		<!-- 需要重连时，连续发出心跳而没有接收到响应的个数（单位：个) -->
		<heartbeat-noresponseout>5</heartbeat-noresponseout>
		<!-- 操作超时时间(单位：秒) -->
		<transaction-timeout>30</transaction-timeout>
		<!--双方协商的版本号(大于0，小于256) -->
		<version>1</version>
		<!--是否属于调试状态,true表示属于调试状态，所有的消息被打印输出到屏幕，false表示不属于调试状态，所有的消息不被输出 -->
		<debug>true</debug>
	</ismg>
</config>
